{
  "name": "lj-utils",
  "version": "0.4.0",
  "description": "uni-app utils",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./microApi": "./src/microApi.ts",
    "./class": "./src/class/index.ts",
    "./resolver.js": "./build/resolver.js",
    "./directive/scrollX": "./src/directive/scrollX.ts",
    "./less/index.less": "./static/less/index.less",
    "./less/common.less": "./static/less/common.less",
    "./scss/index.scss": "./static/scss/index.scss",
    "./scss/global.scss": "./static/scss/global.scss",
    "./scss/animations/index.scss": "./static/scss/animations/index.scss",
    "./package.json": "./package.json"
  },
  "keywords": [
    "utils",
    "uni-app",
    "js",
    "vue"
  ],
  "author": "linjielinlin",
  "email": "993353454@qq.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "less": "npx lessc ./static/less/common.less  ./dist/common.css",
    "sass": "sass --style=expanded --no-source-map ./static/scss/build.scss ./static/defaultCssTip.css",
    "sassMin": "sass --style=compressed --no-source-map ./static/scss/build.scss ./static/defaultCssTip.min.css",
    "rollupC": "rollup -c",
    "tsc": "tsc",
    "esb": "node esbuild.mjs",
    "setVersion": "node --experimental-json-modules --no-warnings changeVersion.js",
    "b": "rm -rf build/* && rollup -c && cp -rf ./static/* ./build && cp -rf *.md ./build/ && node setMetaData && npm run setVersion &&sed -i -r 's/index.ts/index.js/g' ./build/package.json",
    "test": "jest",
    "testWatch": "jest ./test/transform.spec.ts --watch --reporters=\"default\" ",
    "doc": "rm -rf dist/*&&jsdoc -c jsdoc.json && cp static/baidu.js dist/lj-utils/scripts/baidu.js && cp -r ./build/* dist/lj-utils/",
    "build": "npm run b && npm run doc && jest",
    "runDoc": "npm run doc && cd dist && http-server -d dist -p 1000 -c-1 -o",
    "z": "npm version patch && npm run p",
    "y": "npm version minor && npm run p",
    "x": "npm version major && npm run p",
    "p": "./push.sh",
    "push": "nrm use npm && npm login && npm publish && nrm use taobao"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "js-base64": "^3.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/register": "^7.23.7",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.89",
    "cross-env": "^7.0.3",
    "docdash": "^2.0.2",
    "esbuild": "^0.15.18",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.32.0",
    "glob": "^10.3.10",
    "jest": "^27.5.1",
    "jest-html-reporters": "^3.1.7",
    "jsdoc": "^4.0.2",
    "less": "^4.2.0",
    "rollup": "^4.13.0",
    "sass": "^1.72.0",
    "ts-jest": "^27.1.5",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">6"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://gitee.com/uni-pro/utils.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [],
    "extends": [],
    "globals": {
      "window": true,
      "taro": true,
      "uni": true,
      "wx": true,
      "getCurrentPages": true,
      "uniCloud": true
    }
  }
}
